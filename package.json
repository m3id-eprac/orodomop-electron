{
    "homepage": "./",
    "name": "orodomop-electron",
    "version": "0.0.2",
    "description": "my electron app base",
    "main": "dist/main.js",
    "scripts": {
        "build-ts": "swc src -d dist& tsc --pretty --skipLibCheck --noEmit",
        "wbp": "webpack --config ./webpack/webpack.fin.ts",
        "wbp:main": "NODE_DEV=true webpack --config ./webpack/webpack.main.js",
        "wbp:preload": "NODE_DEV=true webpack --config ./webpack/webpack.preload.js",
        "wbp:base": "NODE_DEV=true webpack --config ./webpack/webpack.all.js",
        "wbp:ts": "webpack --config ./webpack/webpack.fin.ts",
        "wbp:watch": "webpack watch",
        "mon": "concurrently 'npm run wbp:watch' 'nodemon --watch . --exec npm run elc'",
        "elc": "NODE_TEST=true electron .",
        "debug": "TEST=true wdio run test/wdio.conf.ts",
        "build": "electron-builder -p never --config ./electron-builder.yml",
        "dev": "(cd server && npm run start) & npm run start",
        "dev:": "concurrently 'cd server && npm run start' 'npm run start'",
        "start:react": "cd ../front && npm run start:none",
        "wait": "concurrently 'npm run start:react' 'wait-on http://localhost:3000 && npm run elc' ",
        "with:react": "concurrently 'cd ../front && npm run start:none' 'NODE_TEST=true npm run elc' ",
        "wdio": "wdio run test/wdio.conf.ts"
    },
    "author": "m3ed-eprac",
    "license": "ISC",
    "devDependencies": {
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.3.0",
        "@swc/jest": "^0.2.22",
        "@testing-library/dom": "^8.17.1",
        "@testing-library/webdriverio": "^3.0.5",
        "@types/js-yaml": "^4.0.5",
        "@types/uuid": "^8.3.4",
        "@types/webpack": "^5.28.0",
        "@types/webpack-node-externals": "^2.5.3",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "@wdio/cli": "^7.24.0",
        "@wdio/local-runner": "^7.24.0",
        "@wdio/mocha-framework": "^7.24.0",
        "@wdio/spec-reporter": "^7.24.0",
        "chromedriver": "^105.0.0",
        "concurrently": "^7.4.0",
        "dotenv": "^16.0.2",
        "electron": "^20.1.4",
        "electron-builder": "^23.3.3",
        "electron-chromedriver": "^20.0.0",
        "eslint": "^8.23.1",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.15.0",
        "eslint-plugin-unicorn": "^43.0.2",
        "eslint-plugin-wdio": "^7.21.0",
        "jest": "^29.0.3",
        "js-yaml": "^4.1.0",
        "nodemon": "^2.0.19",
        "swc-loader": "^0.2.3",
        "ts-jest": "^29.0.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "^4.8.3",
        "wait-on": "^6.0.1",
        "wdio-chromedriver-service": "^7.3.2",
        "wdio-electron-service": "^3.5.0",
        "wdio-wait-for": "^2.2.6",
        "webdriverio": "^7.24.0",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0",
        "webpackbar": "^5.0.2"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "realm": "^10.21.0",
        "uuid": "^9.0.0"
    }
}
